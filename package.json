{
  "name": "xquests-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export -p web",
    "dev": "expo start --web",
    "build:web": "npm run prebuild && expo export -p web --output-dir dist && cp -r public/* dist/ && echo '/* /index.html 200' > dist/_redirects",
    "prebuild": "rm -rf dist",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "deploy": "npm run build:web && netlify deploy --prod --dir=dist"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.1.9",
    "@supabase/supabase-js": "^2.39.0",
    "autoprefixer": "^10.4.20",
    "expo": "~51.0.0",
    "expo-atlas": "^0.4.0",
    "expo-auth-session": "~5.5.2",
    "expo-blur": "~13.0.2",
    "expo-constants": "~16.0.1",
    "expo-crypto": "~13.0.2",
    "expo-font": "~12.0.4",
    "expo-haptics": "~13.0.1",
    "expo-image": "^1.6.0",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.14",
    "expo-secure-store": "~13.0.2",
    "expo-splash-screen": "~0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-symbols": "~0.1.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "lightningcss": "^1.29.2",
    "lucide-react-native": "^0.479.0",
    "nativewind": "^4.1.23",
    "postcss": "^8.5.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "^15.2.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "0.19.10",
    "react-native-webview": "13.8.6",
    "simple-icons": "^15.3.0",
    "tailwindcss": "^3.4.17",
    "tempo-devtools": "^2.0.108",
    "workbox-cli": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.0.8",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.3.0",
    "expo-module-scripts": "^4.1.8",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "private": true
